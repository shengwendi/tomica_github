/* 返回顶部 */
.gotop{
  width:40px;
  height:40px;
  border:1rpx solid rgba(0,0,0,0.1);
  border-radius:50%;
  position:fixed;
  bottom:10px;
  right:10px;
  z-index:99;
  background-color:rgba(255,255,255,0.7);
}

/* 搜索 */
.item-search{
  height:40px; line-height:40px; margin:0 18rpx; padding:0 38px; font-size:28rpx; background:#252525; color:#A7A7A7; border-radius:8px; margin-top:8px; position:relative;
}
.item-search::before{
  content:'';
  position:absolute;
  top:10px;
  left:10px;
  width:20px;
  height:20px;
  background:url(data:image/png;base64,) no-repeat; background-size:20px auto;
}


.search-wrap{
  position:relative;
  display:flex;
  margin-top:8px;
}
.search-wrap .ico-search{
  position:absolute;
  top:9px;
  left:18px;
  width:20px;
  height:20px;
  background:url(data:image/png;base64,) no-repeat; background-size:20px auto;
}
.input-search{
  height:40px; line-height:40px; margin:0 0; padding:0 38px; font-size:28rpx; background:#252525; color:#A7A7A7; border-radius:8px; flex-grow:1; margin-left:18rpx;
}
.btn-search{
  width:68px; height:40px; line-height:40px; color:#FFF; font-size:14px; text-align:center;
}

/* 无结果 */
.search-no{
  font-size:14px; color:#FFF; text-align:center; margin-top:50px;
}
