/**app.wxss**/
view {
  box-sizing: border-box;
  padding: 0;
  margin: 0;
}
.container {
  padding: 0 0 15px; 
  background: #181818;
}
.fn-hide{
  display:none;
}
/* tlv */
.tlv-tit{
  font-size:16px;
  color:#FFF;
  margin:18px 0 0;
  position:relative;
  padding-left:35rpx;
}
.tlv-tit::before{
  content:'';
  position:absolute;
  left:18rpx;
  top:5px;
  width:2px;
  height:12px;
  background:#F75151;
  border-radius:12px;
}
.tlv .ul{
  overflow: hidden; 
  width: 100%;
  padding:0 18rpx;
  display: flex; 
  flex-wrap: wrap;
  justify-content: space-between;
}
.tlv .ul .li{
  width: 340rpx;
  background: #FFFFFF;
  margin-top: 40rpx;
  border-radius: 8rpx;
  overflow: hidden;
}
.tlv .ul .li .thumb{
  width: 340rpx; 
  height: 227rpx;
  overflow:hidden;
  position:relative;
}
.tlv .ul .li .thumb .loading-pic{
  width:20px;
  height:20px;
  display:inline-block;
  vertical-align:middle;
  position:absolute;
  top:92rpx;
  left:147rpx;
  z-index:1;
  -webkit-animation:weuiLoading 1s steps(12, end) infinite;
  animation:weuiLoading 1s steps(12, end) infinite;
  background:transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
.tlv .ul .li image{
  width: 340rpx; 
  height: 227rpx; 
  display: block;
  position:relative;
  z-index:2;
}
.tlv .ul .li .category{
  font-size: 22rpx;
  font-weight: bold;
  color: #fff;
  background-color: #000000;
  padding: 4px 6px;
}
.tlv .ul .li .spec{
  width: 100%;
  margin: 0 auto;
  padding: 20rpx;
}
.tlv .ul .li .spec .h3{
  font-size: 12px;
  font-weight: bold;
  margin-bottom: 5px;
}
.tlv .ul .li .spec .p{
  font-size: 12px;
}
/* 加载更多 */
